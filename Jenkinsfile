                     echo 'Hi, this is Build Phase'
                 }
                 }
                 stage('Test') {
                 steps {
                   echo 'This is Test Phase'
                 }
                 }
		 stage('Deploy') {
                 steps {
                   echo 'This is Deploy Phase'
                 }
                 }
                 
              }
}
